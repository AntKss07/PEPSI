  "section_3_health_goals_detailed": [
    {
      "number": 1,
      "title": "Reverse Fatty Liver (NAFLD Grade II)",
      "key_result": "Improve liver health by reducing inflammation and normalizing liver enzymes",
      "importance": "to protect the liver, improves metabolism, reduces cardiovascular risk, and restores whole-body health",
      "actions": [
        {
          "action": "Add afternoon metabolic movement",
          "details": [
            "10–15 minutes of light walking after lunch and dinner",
            "Avoid prolonged sitting after 3 pm; move 3–5 minutes each hour"
          ],
          "frequency": "6 days a week"
        },
        {
          "action": "Support liver detoxification pathways",
          "details": [
            "Drink 1.5–2.0 liters of water daily",
            "Include cruciferous vegetables (broccoli, cauliflower, Brussels sprouts)",
            "Add leafy greens, garlic, onions, and fresh herbs (cilantro, parsley)"
          ],
          "frequency": "daily"
        }
      ]
    },
    {
      "number": 2,
      "title": "Improve Insulin Sensitivity and Metabolic Flexibility",
      "key_result": "Reduce hepatic insulin resistance and improve metabolic efficiency",
      "importance": "to prevent progression to type 2 diabetes",
      "actions": [
        {
          "action": "Adjust fasting and feeding schedule",
          "details": [
            "Maintain morning fasting but add a small protein-based afternoon snack",
            "Ensure each meal contains protein + fiber"
          ],
          "frequency": "daily"
        },
        {
          "action": "Increase daily fiber intake",
          "details": [
            "1–2 teaspoons of psyllium or ground flaxseed daily",
            "Supports glucose control and liver detoxification"
          ],
          "frequency": "daily"
        },
        {
          "action": "Add low-intensity cardio sessions",
          "details": [
            "12–20 minutes of Zone 2–3 cardio (fast walking, cycling)",
            "Complements strength training routine"
          ],
          "frequency": "3 times a week"
        }
      ]
    },
    {
      "number": 3,
      "title": "Optimize Cardiovascular Risk and Improve Lipid Profile",
      "key_result": "Improve lipid balance and reduce long-term cardiovascular risk",
      "importance": "to prevent progression of atherosclerosis",
      "actions": [
        {
          "action": "Improve fat quality and increase natural omega-3 intake",
          "details": [
            "Eat fatty fish 2–3 times per week (salmon, sardines, tuna)",
            "Limit red meat to 1–2 times per week"
          ],
          "frequency": "as indicated"
        },
        {
          "action": "Reduce Vitamin D supplementation",
          "details": [
            "Lower dose to 2,000–3,000 IU/day",
            "Goal serum level: 50–70 ng/mL"
          ],
          "frequency": "daily"
        },
        {
          "action": "Mandatory post-meal walking",
          "details": [
            "10–12 minutes of walking after the largest meal",
            "Helps reduce postprandial lipids and improves HDL"
          ],
          "frequency": "6 times a week"
        }
      ]
    }
